# Streamlit Secrets Configuration
# Copy the contents below to your Streamlit Cloud app secrets
# (App Settings -> Secrets -> Edit Secrets)

# REQUIRED: Database connection
DATABASE_URL = "postgresql://username:password@host:port/database"

# OPTIONAL: API keys for enhanced features
# If not provided, those features will be disabled gracefully

# For SixtyFour API enrichment features
SIXTYFOUR_API_KEY = "your-sixtyfour-api-key"

# For Claude AI natural language search
ANTHROPIC_API_KEY = "your-anthropic-api-key" 